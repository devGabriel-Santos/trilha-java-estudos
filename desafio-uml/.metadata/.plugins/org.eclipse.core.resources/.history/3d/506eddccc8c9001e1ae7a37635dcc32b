public class Main {

	public static void main(String[] args) {
		
		
		Iphone iphone = new Iphone();
		
		Scanner sc = new Scanner(System.in);
		boolean temRede;
		
		temRede = sc.nextBoolean();
		
		temRede = iphone.redeMovelDisponivel();
		
		
		sc.close();
	}
}
